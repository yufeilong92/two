<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             android:layout_width="match_parent"
                                             android:layout_height="45dp"
                                             android:layout_marginBottom="10dp"
                                             android:layout_marginTop="10dp"
                                             android:layout_marginLeft="12dp"
                                             android:layout_marginRight="12dp"
                                             android:background="@drawable/bg_white5"
>

    <LinearLayout
            android:id="@+id/cell_sendtab_from"
            android:layout_width="0dp"
            android:layout_height="45dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintVertical_chainStyle="spread_inside"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/cell_sendtab_line1"
            android:gravity="center"
            android:orientation="horizontal"
    >
        <TextView
                android:id="@+id/cell_sendtab_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="出发地"
                android:textColor="@color/main_text6"
                android:maxLength="4"
                android:lines="1"
                android:ellipsize="end"
                android:textSize="12sp"
                android:layout_marginRight="6dp"
        />

        <ImageView
                android:id="@+id/cell_mst_next01"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@mipmap/icon_down"/>


    </LinearLayout>

    <View
            android:id="@+id/cell_sendtab_line1"
            android:layout_width="1px"
            android:layout_height="0dp"
            android:background="@color/main_text9"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/cell_sendtab_from"
            app:layout_constraintRight_toLeftOf="@+id/cell_sendtab_to"/>

    <LinearLayout
            android:id="@+id/cell_sendtab_to"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintLeft_toRightOf="@+id/cell_sendtab_line1"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintRight_toLeftOf="@+id/cell_sendtab_line2">

        <TextView
                android:id="@+id/cell_sendtab_to_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="目的地"
                android:textColor="@color/main_text6"
                android:maxLength="4"
                android:lines="1"
                android:ellipsize="end"
                android:textSize="12sp"
                android:layout_marginRight="6dp"
        />

        <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@mipmap/icon_down"/>

    </LinearLayout>

    <View
            android:id="@+id/cell_sendtab_line2"
            android:layout_width="1px"
            android:layout_height="0dp"
            android:background="@color/main_text9"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/cell_sendtab_to"
            app:layout_constraintRight_toLeftOf="@+id/cell_sendtab_px"/>

    <LinearLayout
            android:id="@+id/cell_sendtab_px"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintLeft_toRightOf="@+id/cell_sendtab_line2"
            app:layout_constraintRight_toLeftOf="@+id/cell_sendtab_line3"
            android:gravity="center"
            android:orientation="horizontal"
    >

        <TextView
                android:id="@+id/cell_sendtab_px_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="默认排序"
                android:textColor="@color/main_text6"
                android:maxLength="4"
                android:lines="1"
                android:ellipsize="end"
                android:textSize="12sp"
                android:layout_marginRight="6dp"
        />

        <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@mipmap/icon_down"/>

    </LinearLayout>

    <View
            android:id="@+id/cell_sendtab_line3"
            android:layout_width="1px"
            android:layout_height="0dp"
            android:background="@color/main_text9"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/cell_sendtab_px"
            app:layout_constraintRight_toLeftOf="@+id/cell_sendtab_sx"/>

    <LinearLayout
            android:id="@+id/cell_sendtab_sx"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintLeft_toRightOf="@+id/cell_sendtab_line3"
            app:layout_constraintRight_toRightOf="parent"
            android:gravity="center"
            android:orientation="horizontal"
    >

        <TextView
                android:id="@+id/cell_sendtab_sx_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="筛选"
                android:textColor="@color/main_text6"
                android:maxLength="4"
                android:lines="1"
                android:ellipsize="end"
                android:textSize="12sp"
                android:layout_marginRight="6dp"
        />

        <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:src="@mipmap/icon_down"/>

    </LinearLayout>
</android.support.constraint.ConstraintLayout>