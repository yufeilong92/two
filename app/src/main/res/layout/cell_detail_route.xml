<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             android:layout_width="match_parent"
                                             android:layout_height="wrap_content"
                                             android:background="@drawable/bg_white5">

    <ImageView
            android:id="@+id/cell_droute_icon1"
            android:layout_width="17dp"
            android:layout_height="24dp"
            android:src="@mipmap/icon_location"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="12dp"
    />

    <TextView
            android:id="@+id/cell_droute_path"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="@color/main_text3"
            android:textSize="15sp"
            app:layout_constraintLeft_toRightOf="@+id/cell_droute_icon1"
            app:layout_constraintTop_toTopOf="@+id/cell_droute_icon1"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="8dp"
            android:textStyle="bold"
    />

